{
  "suggest.noselect": false,
  "eslint.filetypes": ["vue"],
  "eslint.options": {
    "extension": [".vue", ".js", ".html", ".jsx"]
  },
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql"
  ],
  "explorer.icon": {
    "expanded": " ",
    "collapsed": " ",
    "enableNerdfont": true
  }
}
